footer.footer
  padding-top 38px
  text-align center

  .icons
    padding-bottom 57px

    img
      display inline-block
      width 94px
      margin 0 4px

  .headline
    @extend .header-description .content .headline
    font-size 50px
    line-height 40px
    padding-bottom 18px

  .subheadline
    @extend .header-description .content .subheadline
    margin-bottom 13px

  .description
    @extend .header-description .text
    max-width 590px
    margin 0 auto

  .share
    padding 60px 0 75px

  .copyrights
    padding 70px 0


.logo
  display inline-block
  sprite($s-logo)
